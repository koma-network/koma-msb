<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.micro.msb.dao.mapper.DaoMapper">			
		
	<resultMap id="msbPropertyReadResult" type="MsbPropertyReadBean">
		<result property="name" column="name"/>
		<result property="propvalue" column="propValue"/>
	</resultMap>
	
	<select id="readMsbProperty" parameterType="com.micro.msb.beans.MsbPropertyReadBean" resultMap="msbPropertyReadResult">         
		select * from msb_property where name = #{name};
	</select>

</mapper>
