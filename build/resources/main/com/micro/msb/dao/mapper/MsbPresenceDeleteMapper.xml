<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.micro.msb.dao.mapper.DaoMapper">			
		
	<resultMap id="msbPresenceDeleteResult" type="MsbPresenceDeleteBean">
		<result property="subscriber" column="subscriber"/>
		<result property="offlinepresence" column="offlinePresence"/>
		<result property="offlinedate" column="offlineDate"/>
	</resultMap>
	
	<delete id="deleteMsbPresence" parameterType="com.micro.msb.beans.MsbPresenceDeleteBean">
		delete from msb_presence where subscriber = #{subscriber};
	</delete>

</mapper>
